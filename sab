-- StarterGui.ScreenGui.PingVex.PingLabel.LocalScript
local function C_125()
local script = G2L["125"];
	local textLabel = script.Parent -- Asegrate de que el script est dentro del TextLabel
	
	local function updatePing()
		local frameCount = 0
		local lastUpdate = tick()
		local ping = 0
	
		-- Bucle de actualizacin del ping
		while true do
			local start = tick()
			pcall(function() game:HttpGet("https://api.roblox.com/robots.txt") end)  -- Calcula el ping
			ping = math.floor((tick() - start) * 1000 + math.random(1, 5))  -- Calcula el ping y agrega un pequeo rango aleatorio para simular fluctuaciones
	
			-- Actualiza el texto del TextLabel con el valor del ping solo
			textLabel.Text = ping .. " MS"  -- Solo muestra el ping (por ejemplo, "50 MS")
	
			-- Cambia el color a rojo si el ping es mayor a 300
			if ping > 300 then
				textLabel.TextColor3 = Color3.fromRGB(255, 0, 0)  -- Rojo para pings mayores a 300 ms
			else
				textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)  -- Blanco para pings ms bajos
			end
	
			-- Espera para no usar demasiados recursos
			wait(1)  -- Actualiza el valor del ping cada segundo
		end
	end
	
	-- Llama a la funcin para comenzar a actualizar el Ping
	updatePing()
	
end;
task.spawn(C_125);
-- StarterGui.ScreenGui.VexInstaGrab.LocalScript
local function C_129()
local script = G2L["129"];
	--=============================--
	--    FRAME ARRASRTABLE + BOTONES
	--=============================--
	
	local guiFrame = script.Parent
	local UserInputService = game:GetService("UserInputService")
	
	guiFrame.Active = true -- Muy importante para recibir Input
	
	local dragging = false
	local dragInput, dragStart, startPos
	
	-- Funcin para actualizar la posicin
	local function update(input)
		local delta = input.Position - dragStart
		guiFrame.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end
	
	-- Detectar inicio de arrastre
	local function inputBegan(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = guiFrame.Position
	
			-- Detectar cuando suelta el mouse
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end
	
	-- Detectar movimiento del mouse
	local function inputChanged(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end
	
	-- Conectar InputChanged global para actualizar mientras arrastra
	UserInputService.InputChanged:Connect(function(input)
		if dragging and input == dragInput then
			update(input)
		end
	end)
	
	-- Conectar eventos de Frame
	guiFrame.InputBegan:Connect(inputBegan)
	guiFrame.InputChanged:Connect(inputChanged)
	
	-- Conectar tambin todos los botones dentro del Frame
	for _, obj in pairs(guiFrame:GetDescendants()) do
		if obj:IsA("GuiButton") then
			obj.InputBegan:Connect(inputBegan)
			obj.InputChanged:Connect(inputChanged)
		end
	end
	
end;
task.spawn(C_129);
-- StarterGui.ScreenGui.VexInstaGrab.TextButton.LocalScript
-- StarterGui.ScreenGui.VexInstaGrab.TextButton.LocalScript
local function C_12b()
local script = G2L["12b"];
    local Players = game:GetService("Players")
    local RunService = game:GetService("RunService")
    local ProximityPromptService = game:GetService("ProximityPromptService")
    local LocalPlayer = Players.LocalPlayer
    
    local button = script.Parent
    local isEnabled = false
    local OFF_COLOR = Color3.fromRGB(255,255,255)
    local ON_COLOR = Color3.fromRGB(0, 255, 0)
    
    -- Variables for auto-grab
    local autoActivate = false
    local MAX_SEARCH_DISTANCE = 6
    local AUTO_ACTIVATE_DELAY = 0.5
    local currentPrompt = nil
    
    -- Function to find nearest steal prompt
    local function findNearestStealPrompt()
        local character = LocalPlayer.Character
        if not character then return nil end
        
        local hrp = character:FindFirstChild("HumanoidRootPart")
        if not hrp then return nil end
        
        local nearestPrompt = nil
        local nearestDistance = MAX_SEARCH_DISTANCE
        
        for _, prompt in ipairs(workspace:GetDescendants()) do
            if prompt:IsA("ProximityPrompt") and prompt.Enabled then
                -- Check if it's a steal prompt
                local actionText = string.lower(prompt.ActionText or "")
                if actionText:find("steal") or actionText:find("grab") or actionText:find("take") then
                    local part = prompt:FindFirstAncestorWhichIsA("BasePart")
                    if part then
                        local distance = (hrp.Position - part.Position).Magnitude
                        if distance <= nearestDistance then
                            nearestPrompt = prompt
                            nearestDistance = distance
                        end
                    end
                end
            end
        end
        
        return nearestPrompt
    end
    
    -- Function to activate the nearest prompt
    local function activateNearestPrompt()
        local prompt = findNearestStealPrompt()
        if prompt then
            currentPrompt = prompt
            fireproximityprompt(prompt, math.huge)
        end
        return prompt ~= nil
    end
    
    -- Auto-activation loop
    local autoActivateConnection = nil
    
    local function startAutoActivation()
        if autoActivateConnection then
            autoActivateConnection:Disconnect()
        end
        
        autoActivateConnection = RunService.Heartbeat:Connect(function()
            if not autoActivate then return end
            
            local prompt = findNearestStealPrompt()
            if prompt then
                fireproximityprompt(prompt, math.huge)
            end
        end)
    end
    
    local function stopAutoActivation()
        if autoActivateConnection then
            autoActivateConnection:Disconnect()
            autoActivateConnection = nil
        end
    end
    
    -- Toggle function
    local function toggleInstaGrab()
        if isEnabled then
            -- Turn OFF
            isEnabled = false
            autoActivate = false
            button.TextColor3 = OFF_COLOR
            stopAutoActivation()
        else
            -- Turn ON
            isEnabled = true
            autoActivate = true
            button.TextColor3 = ON_COLOR
            
            -- Try to activate immediately
            local activated = activateNearestPrompt()
            
            if activated then
                -- Start auto-activation if we found a prompt
                startAutoActivation()
            else
                -- No prompt found, but keep toggle ON and start listening
                startAutoActivation()
            end
        end
    end
    
    -- Also listen for new prompts
    ProximityPromptService.PromptShown:Connect(function(prompt)
        if not isEnabled then return end
        
        local actionText = string.lower(prompt.ActionText or "")
        if actionText:find("steal") or actionText:find("grab") or actionText:find("take") then
            currentPrompt = prompt
            if autoActivate then
                fireproximityprompt(prompt, math.huge)
            end
        end
    end)
    
    -- Connect the button
    button.MouseButton1Click:Connect(toggleInstaGrab)
    
    -- Cleanup when script is destroyed
    script.Destroying:Connect(stopAutoActivation)
    
end;
task.spawn(C_12b);
